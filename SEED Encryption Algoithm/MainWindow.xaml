                            TextWrapping="Wrap"
                            AcceptsReturn="True"
                            MaxHeight="100"      
                            FontSize="18"
  materialDesign:HintAssist.Hint="Блок для зашифрования"
  Style="{StaticResource MaterialDesignFloatingHintTextBox}"
  VerticalAlignment="Top" Margin="57,254,682.2,0" />
                        <TextBox x:Name="EncryptedBlock"
                            TextWrapping="Wrap"

                            AcceptsReturn="True"
