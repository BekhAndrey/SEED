  materialDesign:HintAssist.Hint="Зашифрованный блок"
  Style="{StaticResource MaterialDesignFloatingHintTextBox}"
  VerticalAlignment="Top" Margin="675,254,64.2,0" />
                        <Button x:Name="EncryptBtn" Content="Зашифровать блок" FontSize="18" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="35" Width="287" Click="encryptClick" />
                        <DataGrid x:Name="EncryptGrid" IsReadOnly="True" ToolTipService.ShowDuration="20000" AutoGenerateColumns="False" Height="214" Margin="10,326,10.2,0" VerticalAlignment="Top">
